<html>
  <head>
    <title>Student Dashboard</title>
  </head>
  <body>
    <h1>Welcome to the Student Dashboard</h1>
    <h2>Courses Available for Registration:</h2>

    <!-- Display available courses for registration -->
    <form action="/registercourse" method="POST">
      {{#each availableCourses}}
        <label for="{{this}}">Course {{inc @index}}:</label>
        <input type="text" id="{{this}}" name="{{this}}" required /><br /><br />
      {{/each}}

      <!-- Add more input fields for registering courses -->

      <button type="submit">Register Courses</button>
    </form>

    <h2>Registered Courses:</h2>

    <!-- Display currently registered courses -->
    <ul>
      {{#each registeredCourses}}
        <li>{{this}}</li>
      {{/each}}
    </ul>

    <script>
      Handlebars.registerHelper('inc', function(value, options) { return
      parseInt(value) + 1; });
    </script>
  </body>
</html>